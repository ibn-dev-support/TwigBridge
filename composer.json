{
    "name": "ibn-dev-support/twigbridge",
    "description": "Adds the power of Twig to Laravel",
    "keywords": ["laravel", "twig"],
    "license": "MIT",
    "authors": [
        {
            "name": "iBN Dev Support",
            "email": "dev-support@ibuildnew.com.au"
        }
    ],
    "require": {
        "php": ">=7",
        "twig/twig": "~1.30",
        "illuminate/support": "^5.5|^6|^7",
        "illuminate/view": "^5.5|^6|^7"
    },
    "require-dev": {
        "laravel/framework": "5.5.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^4.0|^7.0|^8.5.8|^9.3.3",
        "squizlabs/php_codesniffer": "~1.5",
        "satooshi/php-coveralls": "~0.6"
    },
    "suggest": {
        "twig/extensions": "~1.0",
        "laravelcollective/html": "For bringing back html/form in Laravel 5.x"
    },
    "autoload": {
        "psr-4": {
            "TwigBridge\\": "src",
            "TwigBridge\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        },
        "laravel": {
            "providers": [
                "TwigBridge\\ServiceProvider"
            ],
            "aliases": {
                "Twig": "TwigBridge\\Facade\\Twig"
            }
        }
    },
    "minimum-stability": "dev"
}
